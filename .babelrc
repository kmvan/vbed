{
    "plugins": [
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-es2015-arrow-functions",
        "lodash",
        [
            "transform-strict-mode",
            {
                "strict": true
            }
        ]
    ],
    "presets": [
        "react",
        "stage-0",
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "last 2 versions",
                        "safari >= 7"
                    ]
                }
            }
        ]
    ]
}
